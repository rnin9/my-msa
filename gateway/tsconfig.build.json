{
  "extends": "./tsconfig.json",
  "exclude": ["node_modules", "test", "dist", "**/*spec.ts"],
  "paths": {
    "@proxy/*": ["features/proxy/*"],
    "@auth/*": ["features/auth/*"],
    "@users/*": ["features/users/*"],
    "@events/*": ["features/events/*"],
    "@rewards/*": ["features/rewards/*"],
    "@rewardRequest/*": ["features/rewardRequest/*"],
    "@eventProgress/*": ["features/eventProgress/*"],
    "@shared/*": ["shared/*"]
  }
}
